{
    "manifest_version": 3,
    "name": "Video Controls",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/watch*",
                "https://www.twitch.tv/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "permission": ["tabs"]

}